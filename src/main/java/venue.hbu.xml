<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.malabanan.localeventuring.Venue" table="venue">
	<id name="venueId" column="VenueId" type="int">
		<generator class="increment"></generator>
	</id>
	<property name="contactId" column="ContactId" type="int"></property>
	<property name="venueName" column="VenueName" type="string"></property>
	<property name="roomSize" column="RoomSize" type="int"></property>
	<property name="capacity" column="Capacity" type="int"></property>
	<property name="price" column="Price" type="int"></property>
	<property name="category" column="Category" type="string"></property>
	<property name="photoLink" column="PhotoLink" type="string"></property>
	<property name="calendarLink" column="CalendarLink" type="string"></property>
	<property name="description" column="Description" type="string"></property>
	<property name="street" column="Street" type="string"></property>
	<property name="city" column="City" type="string"></property>
	<property name="state" column="State" type="string"></property>
	<property name="zipcode" column="Zipcode" type="string"></property>
	
</class>
</hibernate-mapping>